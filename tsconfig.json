{
  "compilerOptions": {
    "module": "esnext",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-native",
    "lib": ["es6"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "es2017",
    "useUnknownInCatchVariables": false,
  },
  "exclude": ["node_modules"],
  "extends": "expo/tsconfig.base"
}
